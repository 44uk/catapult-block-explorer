{
  "name": "catapult-api-rest",
  "version": "1.0.0",
  "description": "",
  "main": "_build/index.js",
  "scripts": {
    "clean": "rimraf _build && mkdir _build",
    "build": "cross-env BABEL_ENV=production babel src -d _build --source-maps",
    "rebuild": "npm run clean && npm run build",
    "start": "node _build/index.js",
    "start:debug": "babel-node src/index.js",
    "test": "mocha --compilers js:babel-core/register -- recursive",
    "test:jenkins": "cross-env JUNIT_REPORT_PATH=test-results.xml mocha --reporter mocha-jenkins-reporter --compilers js:babel-core/register --mongoPort 27018 -- recursive || exit 0",
    "lint": "eslint src test",
    "lint:jenkins": "eslint -o tests.catapult.lint.xml -f junit src test || exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.2",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.0",
    "hippie": "^0.5.1",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "mocha-jenkins-reporter": "^0.3.1",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "catapult-sdk": "~0.2.0",
    "mongodb": "^2.2.10",
    "restify": "^4.3.0",
    "winston": "^2.3.0",
    "ws": "^1.1.1"
  }
}
